{
  "name": "cloudwatch",
  "version": "5.0.0",
  "author": "Cloudreach",
  "contributors": [
    {
      "name": "Kenia Valladarez"
    },
    {
      "name": "Max Eingorn"
    }
  ],
  "private": true,
  "dependencies": {
    "jest-localstorage-mock": "^2.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-materialize": "^2.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "react-test-renderer": "^16.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:lint:js":
      "eslint --fix --ignore-path .gitignore --ext=js --ext=jsx .",
    "test:unit": "react-scripts test --coverage --env=jsdom",
    "test": "run-s test:**",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 0
      }
    }
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.18.2",
    "eslint-config-cloudreach": "^1.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.7.0",
    "npm-run-all": "^4.1.2"
  }
}
